---
import BaseLayout from "../layouts/BaseLayout.astro";
import { getCollection } from "astro:content";

const projects = await getCollection("projects");
console.log("Projects data:", projects.map(p => ({ title: p.data.title, slug: p.slug, id: p.id })));
---

<BaseLayout title="Projects Debug" sideBarActiveItemID="projects">
  <div class="max-w-7xl mx-auto px-8 py-12">
    <h1 class="text-3xl font-bold mb-8">Projects Debug</h1>
    
    <div class="space-y-4">
      {projects.map((project) => (
        <div class="border p-4 rounded">
          <h3 class="font-bold">{project.data.title}</h3>
          <p>ID: {project.id}</p>
          <p>Slug: {project.slug}</p>
          <p>File: {project.id.replace('.md', '')}</p>
        </div>
      ))}
    </div>
  </div>
</BaseLayout>
